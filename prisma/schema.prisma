generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Measurement {
  measure_uuid        String      @id @default(uuid())
  measure_value       Int
  measure_type        String
  customer_code       String
  measure_datetime    DateTime
  created_at          DateTime    @default(now())
  updated_at          DateTime    @updatedAt
}